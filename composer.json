{
    "name": "weisgerber/adsense",
    "type": "typo3-cms-extension",
    "description": "",
    "authors": [
        {
            "name": "Mark Weisgerber",
            "role": "Developer"
        }
    ],
    "license": "GPL-2.0-or-later",
    "require": {
        "typo3/cms-core": "^12|^13"
    },
    "autoload": {
        "psr-4": {
            "Weisgerber\\Adsense\\": "Classes"
        }
    },
    "replace": {
        "typo3-ter/adsense": "self.version"
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin"
    },
    "extra": {
        "typo3/cms": {
            "web-dir": ".Build/public",
            "extension-key": "adsense"
        }
    }
}
